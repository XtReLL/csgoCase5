{
  "compilerOptions": {
    "module": "commonjs",
    "declaration": true,
    "removeComments": true,
    "emitDecoratorMetadata": true,
    "strict": true,
    "experimentalDecorators": true,
    "skipLibCheck": true,
    "allowSyntheticDefaultImports": true,
    "target": "es2017",
    "lib": ["es2019"],
    "sourceMap": true,
    "incremental": true,
    "baseUrl": "."
  }
}
