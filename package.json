  "jest": {
    "moduleDirectories": [
      "node_modules",
      "src"
    ],
    "moduleNameMapper": {
      "^typings/(.*)$": "<rootDir>/../typings/src/$1"
    },
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "restoreMocks": true,
    "testTimeout": 25000,
    "rootDir": "src",
    "testRegex": ".spec.ts$",
    "setupFiles": [
      "<rootDir>/../test/setupTests.ts"
    ],
